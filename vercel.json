{
  "headers": [
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/index.html",
      "destination": "/api/index.js"
    },
    {
      "source": "/",
      "destination": "/api/index.js"
    },
    {
      "source": "/drills",
      "destination": "/api/index.js"
    },
    {
      "source": "/drills/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/routine",
      "destination": "/api/index.js"
    },
    {
      "source": "/routine/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/stats",
      "destination": "/api/index.js"
    },
    {
      "source": "/stats/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    }
  ]
}
